{
    "gitlab": {
        "prod": {
            "url": "http://prod/api/v4",
            "token": "token",
            "clone": "ssh://git@prod"
        },
        "dev": {
            "url": "http://preprod/api/v4",
            "token": "token",
            "clone": "http://preprod",
        },
        "backup": {
